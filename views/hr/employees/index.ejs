<h2>Search Employee</h2>
<form action="/hr/employees" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% employee.forEach(employee => { %>
    <div><%= employee.name %>
        <%= employee.phone %>
        <%= employee.email %>
        <%= employee.address %></div>
<% }) %>